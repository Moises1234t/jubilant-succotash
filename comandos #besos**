module.exports = {
    nombre: 'beso',
    ejecutar: (msg) => {
        if (!msg.mentionedIds || msg.mentionedIds.length === 0) {
            msg.reply('Menciona a quiÃ©n quieres darle un beso. Ejemplo: #beso @usuario');
            return;
